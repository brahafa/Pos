<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/blue_press_2251f3</item>
        <item name="colorPrimaryDark">@color/black_222e3b</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TopText">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_20</item>
        <item name="android:textColor">@color/mine_shaft_282828</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
    </style>

    <style name="TopText.Bold" parent="TopText">
        <item name="android:fontFamily">@font/assistant_bold</item>
    </style>

    <style name="TopTextRole">
        <item name="android:fontFamily">@font/assistant_bold</item>
        <item name="android:textSize">@dimen/text_14</item>
        <item name="android:textColor">@color/mine_shaft_light_2D2D2D</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="TopTextRole.Thin" parent="TopTextRole">
        <item name="android:fontFamily">@font/assistant_regular</item>
    </style>


    <style name="ItemTextStatus">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_18</item>
        <item name="android:lineSpacingMultiplier">1.22</item>
    </style>

    <style name="ItemTextNumber">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_16</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:lineSpacingMultiplier">1.19</item>
    </style>

    <style name="ItemTextBold">
        <item name="android:fontFamily">@font/assistant_semi_bold</item>
        <item name="android:textSize">@dimen/text_24</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="TabText" parent="TextAppearance.Design.Tab">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_18</item>
        <item name="android:lineSpacingMultiplier">1.22</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="TableTextNumber">
        <item name="android:fontFamily">@font/assistant_bold</item>
        <item name="android:textSize">@dimen/text_14</item>
        <item name="android:textColor">@color/selector_table_text_color</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="TableTextStatus">
        <item name="android:fontFamily">@font/assistant_semi_bold</item>
        <item name="android:textSize">@dimen/text_12</item>
        <item name="android:textColor">@color/selector_table_text_color</item>
        <item name="android:lineSpacingMultiplier">1.22</item>
    </style>

    <style name="TableTextNotPaid">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_12</item>
        <item name="android:textColor">@color/mine_shaft_light_2D2D2D</item>
        <item name="android:lineSpacingMultiplier">1.19</item>
    </style>

    <style name="MenuText">
        <item name="android:fontFamily">@font/assistant_bold</item>
        <item name="android:textSize">@dimen/text_24</item>
        <item name="android:textColor">@color/mine_shaft_light_2D2D2D</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="FolderTextThin">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_18</item>
        <item name="android:textColor">@color/mine_shaft_light_2D2D2D</item>
        <item name="android:lineSpacingMultiplier">1.22</item>
    </style>

    <style name="FolderTextBold">
        <item name="android:fontFamily">@font/assistant_bold</item>
        <item name="android:textSize">@dimen/text_18</item>
        <item name="android:textColor">@color/mine_shaft_282828</item>
        <item name="android:lineSpacingMultiplier">1.22</item>
    </style>

    <style name="TitleText">
        <item name="android:fontFamily">@font/assistant_bold</item>
        <item name="android:textSize">@dimen/text_24</item>
        <item name="android:textColor">@color/mine_shaft_282828</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="TitleTextThin">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_20</item>
        <item name="android:textColor">@color/mine_shaft_light_2D2D2D</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="ToppingText">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_20</item>
        <item name="android:textColor">@color/selector_topping_color</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
    </style>

    <style name="DrinkText">
        <item name="android:fontFamily">@font/assistant_bold</item>
        <item name="android:textSize">@dimen/text_24</item>
        <item name="android:textColor">@color/selector_topping_color</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
    </style>


    <style name="PizzaNum">
        <item name="android:fontFamily">@font/assistant_bold</item>
        <item name="android:textSize">@dimen/text_14</item>
        <item name="android:textColor">@color/burning_orange_FB6D3A</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="BottomButtonsText">
        <item name="android:fontFamily">@font/assistant_bold</item>
        <item name="android:textSize">@dimen/text_24</item>
        <item name="android:textColor">@color/selector_text_color_disabled_send</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="BottomButtonsTextThin">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_24</item>
        <item name="android:textColor">@color/selector_text_color_disabled</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="CartTopText">
        <item name="android:fontFamily">@font/assistant_bold</item>
        <item name="android:textSize">@dimen/text_20</item>
        <item name="android:textColor">@color/mine_shaft_282828</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="CartText">
        <item name="android:fontFamily">@font/assistant_bold</item>
        <item name="android:textSize">@dimen/text_24</item>
        <item name="android:textColor">@color/mine_shaft_282828</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="CartText.Thin" parent="CartText">
        <item name="android:fontFamily">@font/assistant_regular</item>
    </style>

    <style name="CartText.Big" parent="CartText">
        <item name="android:textSize">@dimen/text_32</item>
    </style>

    <style name="PaymentTitle">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_24</item>
        <item name="android:textColor">@color/mine_shaft_282828</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

