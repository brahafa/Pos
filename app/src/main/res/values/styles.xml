    </style>

    <style name="EditTextText">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_24</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/santas_gray_A3A3A4</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="TimeText">
        <item name="android:fontFamily">@font/assistant_extra_bold</item>
        <item name="android:textSize">@dimen/text_120</item>
        <item name="android:textColor">@color/santas_gray_A3A3A4</item>
        <item name="android:lineSpacingMultiplier">1.31</item>
