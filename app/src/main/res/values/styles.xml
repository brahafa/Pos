    </style>

    <style name="ButtonsText">
        <item name="android:fontFamily">@font/assistant_bold</item>
        <item name="android:textSize">@dimen/text_20</item>
        <item name="android:textColor">@color/selector_text_color_disabled_send</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="MonthText">
        <item name="android:fontFamily">@font/assistant_regular</item>
        <item name="android:textSize">@dimen/text_20</item>
        <item name="android:textColor">@color/selector_text_color_month</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="TimesText">
        <item name="android:fontFamily">@font/assistant_semi_bold</item>
        <item name="android:textSize">@dimen/text_24</item>
        <item name="android:textColor">@color/mine_shaft_light_2D2D2D</item>
        <item name="android:lineSpacingMultiplier">1.21</item>
    </style>

    <style name="EditTimesText" parent="TimesText">
        <item name="android:fontFamily">@font/assistant_regular</item>
    </style>


</resources>
